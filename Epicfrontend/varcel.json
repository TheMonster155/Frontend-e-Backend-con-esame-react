{
    "version": 2,
    "builds": [
      {
        "src": "Epicfrontend/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "epicbackend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "epicbackend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Epicfrontend/$1"
      }
    ]
  }
  